{
    "name": "Interest Integrator",
    "description" : "A chrome extension to help you integrate your interests",
    "version": "0.1.0",
    "manifest_version": 2,
    "background": {
        "persistent": false,
        "scripts": ["background.js"]
      },
      "browser_action": {
        "default_title": "Integrate interests."
      },
      "permissions": [
        "storage"
      ]   
  }